#include <iostream>
using namespace std;
int main() {
    int num1, num2, result;
    cout << "Enter the first number: ";
    cin >> num1;
    cout << "Enter the second number: ";
    cin >> num2;
    result = num1-num2
    cout << "The difference between " << num1 << " and " << num2 << " is: " << result << endl;
    return 0;
}
