#include <iostream>
using namespace std;
int main() {
    int n, num;
    int sum_even = 0, sum_odd = 0;
    cout << "Enter the number of elements: ";
    cin >> n;
    cout << "Enter " << n << " numbers: ";
    for (int i = 0; i < n; ++i) {
        cin >> num;
        if (num % 2 == 0) {
            sum_even += num; 
        } else {
            sum_odd += num; 
        }
    }
    cout << "Sum of even numbers: " << sum_even << endl;
    cout << "Sum of odd numbers: " << sum_odd << endl;
    return 0;
}
