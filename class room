#include <iostream>

class Room
{
public:
double length;
 double breadth;
double height;
 double calculateArea()
   {
 return length * breadth;
  }
 double calculateVolume()
 {
 return length * breadth * height;
 }
   };
  int main()
 {
 room1.length = 42.5;
 room1.breadth = 30.8;
 room1.height = 19.2;
 std::cout << "Area of Room: " << room1.calculateArea() << std::endl;
 std::cout << "Volume of Room: " << room1.calculateVolume() << std::endl;
  return 0;
}
